package actividad_evaluable;

import java.util.Arrays;

public class Usuario {
	private String nombre;
	private String apellidos;
	private Genero genero;
	private String usuario;
	private String telefono;
	private String destino;
	private String[] vacunas;

	public Usuario(String nombre, String apellidos, Genero genero, String usuario, String telefono, String destino, String[] vacunas) {
		this.nombre = nombre;
		this.apellidos = apellidos;
		this.genero = genero;
		this.usuario = usuario;
		this.telefono = telefono;
		this.destino = destino;
		this.vacunas = vacunas;
	}

	@Override
	public String toString() {
		return "Usuario [nombre=" + nombre + ", apellidos=" + apellidos + ", genero=" + genero + ", usuario=" + usuario
				+ ", telefono=" + telefono + ", destino=" + destino + ", vacunas=" + Arrays.toString(vacunas) + "]";
	}

	
	
}
